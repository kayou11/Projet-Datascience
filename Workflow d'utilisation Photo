emin = \"/content/set\" #@param {type:\"string\"}\n","\n","path = chemin\n","\n","start_time = time.time()\n","lancement_processus(path, True)\n","print(\"Temps d execution : %s secondes ---\" % (time.time() - start_time)) # Affichage du temps d'exécution\n","'''"],"execution_count":0,"outputs":[]}]}